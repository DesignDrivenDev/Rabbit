{
    "version":2,
    "name":"backend",
    "builds":[
        {
            "src":"server.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "handle":[
                {
                    "src":"/(.*)",
                    "dest":"server.js"
                }
            ]
        }
    ]
}